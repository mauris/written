graph {
  nodesep=0.8;
  ranksep=0.5;
  node[shape=circle];
  
  0 [label="x"];
  1 [label="y"];
  2 [label="y"];
  
  0 -- 1 [style="dashed"];
  0 -- 2 [style="solid"];
  
  node[shape=square];
  
  3 [label="1"];
  4 [label="0"];
  
  1 -- 3 [style="dashed"];
  1 -- 4 [style="solid"];
  
  5 [label="0"];
  6 [label="0"];
  
  2 -- 5 [style="dashed"];
  2 -- 6 [style="solid"];
  
  node[style=invis];
  edge[style=invis]
  x1;
  1 -- x1 -- 2;
  
  { rank=same; 1; x1; 2;}
}