digraph {
  rankdir=LR;
  node [shape=point, style=invis];
  start;
  middle;
  
  node [shape=doublecircle, style=solid];
  q0;
  
  node [shape=circle, style=solid];
  q1;
  
  edge [splines=true];
  
  start -> q0;
  q0 -> q0 [label="0"];
  q0 -> q1 [label="1"];
  q1 -> q1 [label="0"];
  q1 -> q0 [label="1"];
}