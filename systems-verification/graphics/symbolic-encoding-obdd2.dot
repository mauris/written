graph {
  nodesep=0.8;
  ranksep=0.5;
  
  
  node[shape=circle];
  x1_1 [label="x1"];
  x2_1 [label="x2"];
  x2_2 [label="x2"];
  
  node[shape=square];
  
  0 [label="0"]
  1 [label="1"]
  
  x1_1 -- x2_1 [style="dotted"];
  x1_1 -- x2_2 [style="solid"];
  x2_1 -- 1 [style="solid"];
  x2_1 -- 0 [style="dotted"];
  
  x2_2 -- 0 [style="solid"];
  x2_2 -- 1 [style="dotted"];
  
}